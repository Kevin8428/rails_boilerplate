<%= form_for(@test)do|f| %>
  <%= f.label 'title' %>
  <%= f.text_field :title %><br>
  <%= f.label 'desc' %>
  <%= f.text_field :description %><br>
  <%= f.label 'account' %>
  <!-- <%= f.text_field :account_id, value: '100' %><br> -->
  <%= f.text_field :account_id, value: session[:user_id] %><br>
  <%= f.submit %>
<% end %>
<%=session[:user_id]%>
